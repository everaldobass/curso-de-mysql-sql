--Exemplos
SELECT * FROM Funcionarios

-- Exibe tofod os dados dos funcionarios da cidade de Caruaru com cargo de Gerente
SELECT *
FROM Funcionarios
WHERE cidade = 'Caruaru' AND cargo = 'Gerente'

-- Exibe todos os dados dos funcionarios da cidade de S達o Paulo ou cargo de TI
SELECT *
FROM Funcionarios
WHERE cidade = 'Jundiai' AND cargo = 'TI'

-- todos os dados dos funcionarios que n達o s達o do estado de S達o Paulo
SELECT *
FROM Funcionarios
WHERE NOT estado = 'SP'

